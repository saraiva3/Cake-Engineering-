<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\InvestimentoTotal" table="investimento_total">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="resultado" type="string" column="resultado" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="percentualRecursosproprios" type="integer" column="percentual_recursosProprios" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="percentualRecursosterceiros" type="integer" column="percentual_recursosTerceiros" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="percentualOutros" type="integer" column="percentual_outros" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
