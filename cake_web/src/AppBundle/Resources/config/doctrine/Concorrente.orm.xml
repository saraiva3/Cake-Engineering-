<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Concorrente" table="concorrente">
    <indexes>
      <index name="fk_concorrente_plano_negocio1_idx" columns="plano_negocio_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nome" type="string" column="nome" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="localizacao" type="string" column="localizacao" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="atendimento" type="string" column="atendimento" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="preco" type="string" column="preco" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="qualidade" type="string" column="qualidade" length="450" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="servicosClientes" type="string" column="servicos_clientes" length="450" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="condicoesPagamentos" type="string" column="condicoes_pagamentos" length="450" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="planoNegocio" target-entity="PlanoNegocio" fetch="LAZY">
      <join-columns>
        <join-column name="plano_negocio_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
