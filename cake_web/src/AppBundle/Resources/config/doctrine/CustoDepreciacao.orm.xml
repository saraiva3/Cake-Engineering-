<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CustoDepreciacao" table="custo_depreciacao">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="resultado" type="float" column="resultado" precision="10" scale="0" nullable="true"/>
    <field name="descricao" type="string" column="descricao" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valor" type="float" column="valor" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
